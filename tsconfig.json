{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ES6",
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "@styles/*": ["src/styles/*"],
      "@modules/*": ["src/modules/*"],
      "@redux/*": ["src/redux/*"]
    }
  }
}
